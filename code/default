CC = gcc

objects = ../data_list.o ../print_funcs.o \
../data_stream.o ../common.o

WARNINGS = -Wall -Wextra -Werror -Wdeclaration-after-statement \
-Wshadow -Wstrict-overflow=5 -Wpointer-arith -Wcast-qual \
-Wstrict-prototypes -Wmissing-prototypes -Wmissing-declarations \
-Winit-self -Wcast-align -Wformat=2 -Wmissing-include-dirs \
-Wunused-parameter

CFLAGS = $(WARNINGS) -c -pedantic -ansi -g

